dom-module#cs-system-admin-permissions-form
	template
		form(is="cs-form")
			template(is="dom-if" if="[[permission_id]]")
				label id
				cs-input-text: input(readonly value="[[permission_id]]")
			label [[L.group]]
			cs-input-text: input(type="text" value="{{group}}")
			label [[L.label]]
			cs-input-text: input(type="text" value="{{label}}")
			div
				cs-button(primary): button(type="button" on-tap="save" disabled="[[!and(group, label)]]") [[L.save]]
	script(src="script.js")
