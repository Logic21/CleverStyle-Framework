{
	"package"      : "Static_pages",
	"category"     : "modules",
	"version"      : "1.54.0+build-142",
	"description"  : "Creating of simple static pages with categories",
	"author"       : "Nazar Mokrynskyi",
	"website"      : "cleverstyle.org/Framework",
	"license"      : "MIT License",
	"assets"       : {
		"admin/Static_pages" : [
			"cs-static-pages-admin-*"
		],
		"Static_pages"       : [
			"cs-static-pages-*"
		]
	},
	"db"           : [
		"pages"
	],
	"db_support"   : [
		"MySQLi"
	],
